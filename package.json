{
  "name": "monero-miner",
  "version": "0.0.0-development",
  "license": "MIT",
  "files": ["dist", "xmrig/build"],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "format": "run-p prettier \"lint --format\"",
    "lint": "tslint --project tsconfig.json --format codeFrame",
    "jest": "jest",
    "prepare": "run-p build test",
    "prebuild": "run-s format",
    "precommit": "run-s prepare",
    "prettier": "prettier *.json --write",
    "build": "tsc --pretty",
    "test": "run-p jest lint",
    "watch": "tsc --pretty --watch",
    "watch:test": "jest --watch",
    "compile": "./build.sh",
    "semantic-release":
      "semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "@commitlint/cli": "^5.2.0",
    "@commitlint/config-conventional": "^5.1.3",
    "@types/jest": "^21.1.8",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-environment-node-debug": "^2.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.5.2",
    "rimraf": "^2.0.0",
    "semantic-release": "^8.2.0",
    "ts-jest": "^21.2.3",
    "ts-node": "^4.0.1",
    "tslint": "^5.0.0",
    "tslint-config-lemon": "^1.1.7",
    "typescript": "^2.3.0"
  },
  "dependencies": {
    "@types/execa": "^0.8.0",
    "@types/node": ">=8.0.0",
    "execa": "^0.8.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "transform": {
      ".(ts)": "ts-jest"
    },
    "testRegex": "/__tests__/.*.test.ts$",
    "moduleFileExtensions": ["ts", "js", "json"],
    "testEnvironment": "node",
    "mapCoverage": true
  },
  "precommit": "prepare",
  "description": "monero-miner",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yellowiki/monero-miner.git"
  }
}
